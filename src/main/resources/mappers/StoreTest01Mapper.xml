<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.penguin.spring.test.datebase.repository.StoreTest01Repository">
 	<!-- 메소드 연걸  -->
 	<select id ="selectStoreTest01List" 
 	resultType="com.penguin.spring.test.datebase.domain.StoreTest01">
 		SELECT
 			`id`
 			,`name`
 			,`phoneNumber`
 			,`address`
 			,`businessNumber`
 			,`introduce`
 			,`createdAt`
 			,`updatedAt`
 		FROM
 			`store`
 	</select>
 
 </mapper>